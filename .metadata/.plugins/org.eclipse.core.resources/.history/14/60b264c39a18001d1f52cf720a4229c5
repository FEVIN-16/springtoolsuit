package com.hexaware.collectionsandjava8;

import java.util.ArrayList;
import java.util.List;
public class TestSeller {

	public static void main(String[] args) {
		Seller se=new Seller();
		List<String> arr = new ArrayList<>();
		se.setSellerId(01);
		se.setSellerName("Fevin");
		se.setSellerEmail("fevin@gmail.com");
		se.setSellerLocation("Kanyakumari");
		
		arr.add(String.valueOf(se.getSellerId()));
		arr.add(se.getSellerName());
		arr.add(se.getSellerEmail());
		arr.add(se.getSellerLocation());
		
		se.setSellerId(02);
		se.setSellerName("Fejin");
		se.setSellerEmail("fejinfm2000@gmail.com");
		se.setSellerLocation("Kanyakumari");
		
		arr.add(String.valueOf(se.getSellerId()));
		arr.add(se.getSellerName());
		arr.add(se.getSellerEmail());
		arr.add(se.getSellerLocation());
		
		se.setSellerId(02);
		se.setSellerName("Vibi");
		se.setSellerEmail("vibi@gmail.com");
		se.setSellerLocation("Chennai");
		
		arr.add(String.valueOf(se.getSellerId()));
		arr.add(se.getSellerName());
		arr.add(se.getSellerEmail());
		arr.add(se.getSellerLocation());
		
		se.setSellerId(02);
		se.setSellerName("Elsie");
		se.setSellerEmail("elsie@gmail.com");
		se.setSellerLocation("Chennai");
		
		arr.add(String.valueOf(se.getSellerId()));
		arr.add(se.getSellerName());
		arr.add(se.getSellerEmail());
		arr.add(se.getSellerLocation());
		
		System.out.println("Displaying elements");
		arr.stream()
			.forEach(System.out::println);
		
		
		
		arr.stream()
			.map(n ->n.toLowerCase())
			.forEach(System.out::println);
		

	}

}
